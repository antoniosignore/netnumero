<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:gwtdate="com.google.gwt.user.datepicker.client"
        xmlns:nhw="urn:import:com.numhero.client.widget"
        xmlns:gwt="urn:import:com.google.gwt.user.client.ui"
        xmlns:nhex="urn:import:com.numhero.client.mvp.report.trialbalance"
        ui:defaultLocale="en"
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
        ui:generateFilename="TrialBalancePanel" ui:generateLocales="default">


    <gwt:HTMLPanel styleName="formContainer">

        <gwt:HTMLPanel styleName="form-with-shadow">

            <table class="formContainer" cellpadding="0" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <td colspan="14">
                            <ui:msg key="trial.balance">Trial Balance</ui:msg>
                        </td>
                    </tr>
                </thead>
                <tbody>

                    <tr class="formContainer" cellpadding="0" cellspacing="0" width="100%">
                        <td colspan="14">
                            <nhex:TrialBalanceTable ui:field="trialBalanceTable"/>
                        </td>
                    </tr>
                </tbody>
            </table>
        </gwt:HTMLPanel>
    </gwt:HTMLPanel>
</ui:UiBinder>
